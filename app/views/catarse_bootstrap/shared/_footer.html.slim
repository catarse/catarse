footer.w-hidden-small.w-hidden-tiny.footer-simple
  .w-container
    .footer-signature-text.caption #{CatarseSettings[:company_name]} #{Time.now.strftime("%Y")}
    ul.w-list-unstyled.footer-simple-links.caption
      li.footer-simple-links-list = link_to t('footer.links.blog'), CatarseSettings[:blog_url], class: "link-footer"
      li.footer-simple-links-list = link_to t('footer.links.discover'), explore_path, class: "link-footer"
      li.footer-simple-links-list = link_to t('footer.links.start'), start_path, class: "link-footer"
      li.footer-simple-links-list = link_to t('footer.links.faq'), CatarseSettings[:faq_url], class: "link-footer"
      li.footer-simple-links-list = link_to t('footer.links.feedback'), CatarseSettings[:feedback_url], class: "link-footer"
      li.footer-simple-links-list = link_to t('footer.links.terms'), terms_of_use_path, class: "link-footer"
      li.footer-simple-links-list = link_to t('footer.links.privacy'), privacy_policy_path, class: "link-footer"
